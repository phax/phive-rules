								</xsd:annotation>
								<xsd:complexType>
									<xsd:sequence minOccurs="0">
										<xsd:element name="StopkaFaktury" type="etd:TTekstowy" minOccurs="0">
											<xsd:annotation>
												<xsd:documentation>Stopka faktury</xsd:documentation>
											</xsd:annotation>
										</xsd:element>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="Rejestry" minOccurs="0" maxOccurs="100">
