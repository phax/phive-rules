<?xml version="1.0" encoding="UTF-8"?>
<!--BII2-T19-R012-->
<testSet xmlns="http://difi.no/xsd/vefa/validator/1.0"
	configuration="peppolbis-t16-base-3.0">
	<assert>
		<description>Verify DespatchAdvice supplier name or identifier is specified</description>
		<scope>PEPPOL-T16-R010</scope>
	</assert>
	
	<test id="1">
		<assert>
			<success>PEPPOL-T16-R010</success>
		</assert>
		<DespatchAdvice xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" 
				xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2" 
				xmlns="urn:oasis:names:specification:ubl:schema:xsd:DespatchAdvice-2">
			
			<cac:OriginatorCustomerParty>
				<cac:Party>
					<cac:PartyIdentification>
						<cbc:ID schemeID="0088">5790000435951</cbc:ID>
					</cac:PartyIdentification>
					<cac:PartyName>
						<cbc:Name>ABC Supplier Ltd.</cbc:Name>
					</cac:PartyName>
				</cac:Party>
			</cac:OriginatorCustomerParty>

		</DespatchAdvice>
	</test>

	<test id="2">
		<assert>
			<success>PEPPOL-T16-R010</success>
		</assert>
		<DespatchAdvice xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" 
				xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2" 
				xmlns="urn:oasis:names:specification:ubl:schema:xsd:DespatchAdvice-2">
			
			<cac:OriginatorCustomerParty>
				<cac:Party>
					<cac:PartyIdentification>
						<cbc:ID schemeID="0088">5790000435951</cbc:ID>
					</cac:PartyIdentification>
				</cac:Party>
			</cac:OriginatorCustomerParty>

		</DespatchAdvice>
	</test>
	
	<test id="3">
		<assert>
			<success>PEPPOL-T16-R010</success>
		</assert>
		<DespatchAdvice xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" 
				xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2" 
				xmlns="urn:oasis:names:specification:ubl:schema:xsd:DespatchAdvice-2">
			
			<cac:OriginatorCustomerParty>
				<cac:Party>
					<cac:PartyName>
						<cbc:Name>ABC Supplier Ltd.</cbc:Name>
					</cac:PartyName>
				</cac:Party>
			</cac:OriginatorCustomerParty>

		</DespatchAdvice>
	</test>
	
	<test id="4">
		<assert>
			<error>PEPPOL-T16-R010</error>
		</assert>
		<DespatchAdvice xmlns:cac="urn:oasis:names:specification:ubl:schema:xsd:CommonAggregateComponents-2" 
				xmlns:cbc="urn:oasis:names:specification:ubl:schema:xsd:CommonBasicComponents-2" 
				xmlns="urn:oasis:names:specification:ubl:schema:xsd:DespatchAdvice-2">
			
			<cac:OriginatorCustomerParty>
			</cac:OriginatorCustomerParty>

		</DespatchAdvice>
	</test>

</testSet>